{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": true,  // Set to true if needed
    "anonymousAuthentication": false,  // Set to false for increased security
    "iisExpress": {
      "applicationUrl": "http://localhost:5001", // Change to your production URL
      "sslPort": 443 // Change to your production SSL port
    }
  },
  "profiles": {
    "production": {
      "commandName": "Project",
      "dotnetRunMessages": false,
      "launchBrowser": false, // Set to false in production
      "launchUrl": "swagger", // Change to the default URL you want to launch
      "applicationUrl": "https://localhost:5000;http://localhost:5001", // Change to your production URL
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production" // Change to "Production" in production
      }
    }
  }
}
